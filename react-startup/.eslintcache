"react\";\nimport Beginner from \"./components/beginner\";\nimport Counter from \"./components/counter\";\nimport List from \"./components/list\";\nimport Foods from \"./components/foods\";\nimport Counters from \"./components/counters\";\nimport Todo from \"./components/todo\";\n\nfunction App() {\n\treturn (\n\t\t<>\n\t\t\t<Todo/>\n\t\t</>\n\t);\n}\n\nexport default App;",["62","63"],"C:\\Users\\acer\\Desktop\\react-startUp\\react-startup\\src\\components\\counter.jsx",["64"],"import React, { Component } from 'react'\r\nimport { Modal, Button } from 'antd';\r\n\r\nclass Counter extends Component {\r\n    constructor () {\r\n        super();\r\n        this.state = {\r\n            todo: \"\",\r\n            todos: [],\r\n            id: 1,\r\n            complete: false,\r\n            loading: false,\r\n            visible: false,\r\n        };\r\n    }\r\n    showModal = () => {\r\n        this.setState({\r\n          visible: true,\r\n        });\r\n      };\r\n    \r\n      handleOk = () => {\r\n        this.setState({ loading: true });\r\n        setTimeout(() => {\r\n          this.setState({ loading: false, visible: false });\r\n        }, 3000);\r\n      };\r\n    \r\n      handleCancel = () => {\r\n        this.setState({ visible: false });\r\n      };\r\n    handleInputChange = (e) => {\r\n       this.setState(\r\n        {todo: e.target.value} \r\n       )\r\n    }\r\n    addTodo = () =>{\r\n        if (this.state.todo !== \"\"){\r\n            let todo = {\r\n                todo: this.state.todo,\r\n                id: this.state.id,\r\n            }\r\n            this.setState({\r\n                todos: this.state.todos.concat(todo),\r\n                id: this.state.id + 1,\r\n                todo: \" \"\r\n            })\r\n        }\r\n        else {\r\n            alert(\"yo'q\")\r\n        }\r\n    }\r\n    deleteTodo = (index) =>{\r\n        this.setState({\r\n            todos: this.state.todos.filter((item) =>{\r\n                if(item.id !== index)\r\n                return item\r\n            })\r\n        })\r\n    }\r\n    onclickCheck = (id) => {\r\n        this.setState({\r\n            todos: this.state.todos.filter((item)=>{\r\n                if (item.id === id)\r\n                    item.complete =! item.complete\r\n                    return item\r\n            })\r\n        })\r\n    }\r\n    render() { \r\n        const { visible, loading } = this.state;\r\n        return(  \r\n            <div className=\"container\">\r\n                <div className=\"row justify-content-center mt-5\">\r\n                    <div className=\"col-md-8 d-flex justify-content-between\">\r\n                        <input type=\"text\" placeholder=\"Ma'lumot kiriting...\" value={this.state.todo} className=\"form-control border-0\" onChange={this.handleInputChange}/>\r\n                        <button type=\"button\" className=\"btn bg-danger text-white ml-3\" onClick={this.addTodo}>Add</button>\r\n                    </div>\r\n                </div>\r\n                <div className=\"row justify-content-center mt-5\">\r\n                    <div className=\"col-md-8 justify-content-between\">\r\n                    {this.state.todos.map((item) => {\r\n                        var a = <div className=\"col-6\"><div className=\"box\">{item.todo}</div></div>\r\n                        var b = <div className=\"col-6\"><div className=\"box box2\">{item.todo}</div></div>\r\n                        return <div className=\"row d-flex justify-content-start mt-3\">\r\n                            <div className=\"col-2\"><div className=\"bg-dark\"><p>{item.id}</p></div>\r\n                            </div>\r\n                            {item.complete ? b : a}\r\n                            <div className=\"col-4 d-flex\">\r\n                                <div className=\"bg-dark\" onClick={() => this.onclickCheck(item.id)}><p><i className=\"fas fa-check\"></i></p></div>\r\n                                <div className=\"bg-dark\" onClick={this.showModal}><p><i className=\"fas fa-pen-nib\"></i></p></div>\r\n                                <Modal\r\n                                    visible={visible}\r\n                                    title=\"Title\"\r\n                                    onOk={this.handleOk}\r\n                                    onCancel={this.handleCancel}\r\n                                    footer={[\r\n                                    <Button key=\"back\" onClick={this.handleCancel}>\r\n                                        Cancel\r\n                                    </Button>,\r\n                                    <Button key=\"submit\" type=\"primary\" loading={loading} onClick={this.handleOk}>\r\n                                        Ok\r\n                                    </Button>,\r\n                                    ]}\r\n                                    >\r\n                                    <p>06:00 da uyg'on!!!</p>\r\n                                </Modal>\r\n                                <div className=\"bg-dark\" onClick={() => this.deleteTodo(item.id)}><p><i className=\"fas fa-times\"></i></p></div>\r\n                            </div>\r\n                        </div>\r\n                    })}\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n \r\nexport default Counter;\r\n\r\n","C:\\Users\\acer\\Desktop\\react-startUp\\react-startup\\src\\components\\QuickUp\\quickUp.jsx",[],"C:\\Users\\acer\\Desktop\\react-startUp\\react-startup\\src\\components\\beginner.jsx",[],"C:\\Users\\acer\\Desktop\\react-startUp\\react-startup\\src\\components\\list.jsx",[],"C:\\Users\\acer\\Desktop\\react-startUp\\react-startup\\src\\components\\foods.jsx",[],"C:\\Users\\acer\\Desktop\\react-startUp\\react-startup\\src\\services\\fakeFoods.js",[],"C:\\Users\\acer\\Desktop\\react-startUp\\react-startup\\src\\services\\fakeCategories.js",[],"C:\\Users\\acer\\Desktop\\rea